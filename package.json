{
  "name": "@idio/frontend",
  "version": "1.5.0",
  "description": "The Middleware To Serve Front-End JavaScript.",
  "main": "build/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc documentary -o README.md",
    "b": "alamode src -o build -s",
    "d": "yarn-s d1",
    "d1": "NODE_DEBUG=doc doc src/index.js -g",
    "build": "yarn-s d b doc",
    "rec": "NODE_DEBUG=appshot appshot -T 23 -a Terminal -y 150 -f",
    "e": "alanode",
    "example/": "yarn e example/example"
  },
  "files": [
    "build",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/idiocc/frontend.git"
  },
  "keywords": [
    "frontend",
    "idio",
    "jsx",
    "node_modules",
    "browser",
    "development",
    "dev",
    "middleware"
  ],
  "author": "Anton <anton@adc.sh>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/idiocc/frontend/issues"
  },
  "homepage": "https://github.com/idiocc/frontend#readme",
  "devDependencies": {
    "@depack/render": "^1.1.4",
    "@idio/core": "^1.3.6",
    "@idio/preact-fixture": "1.0.0",
    "alamode": "^1.9.3",
    "documentary": "^1.23.4",
    "eslint-config-artdeco": "1.0.1",
    "preact": "8.4.2",
    "rqt": "^3.1.3",
    "temp-context": "^2.1.3",
    "yarn-s": "1.1.0",
    "zoroaster": "^3.11.5"
  },
  "dependencies": {
    "@a-la/jsx": "^1.4.7",
    "@wrote/exists": "^1.0.3",
    "@wrote/read": "^1.0.4",
    "catchment": "^3.3.0",
    "fpj": "^1.1.0",
    "makepromise": "^3.1.0",
    "resolve-dependency": "^1.0.2",
    "restream": "^3.6.0"
  }
}
